{
    "template": {
        "type": "selector",
        "value": ".template",
        "binds": [
            {
                "target": ".delete",
                "on": [{
                    "name": "click",
                    "handler": "removeItem"
                }]
            },
            {
                "on": [{
                    "name": "click",
                    "handler": "selectItem"
                }]
            },
            {
                "attr": [
                    {
                        "name": "id",
                        "value": {
                            "source": "id",
                            "filter": [
                                { "name": "prefix", "args": "itm", "library": "/aasdsda" }
                            ]
                        }
                    }
                ]
            },
            {
                "target": ".itemText",
                "html": {
                    "source": "text"
                }
            }
        ]
    },
    "container": ".content",
    "crud": {
        "create": "create",
        "read":   "read",
        "update": "update",
        "delete": "remove"
    },
    "options": {
        "selection": "multiple",
        "autofetch": true,
        "classes": {
            "selected": "selected"
        },
        "id": "id",
        "pagination": {
            "size": 2,
            "container": ".pagination",
            "numbers": {
                "template": ".pagination-template",
                "classes": {
                    "active": "active",
                    "item": "pagination-item",
                    "before": ".prepend-before"
                },
                "keywords": {
                    "pageNumber": "@pageNr@",
                    "active": "@active@"
                },
                "options": {
                    "max": 4,
                    "alwaysFirst": true,
                    "alwaysLast": true
                }
            },
            "controls": {
                "next": ".next",
                "previous": ".prev",
                "disable": "disabled"
            },
            "classes": {
                "disable": "disabled"
            }
        }
    },
    "controls": {
        "add": ".create",
        "delete": ".delete"
    },
    "binds": [
        {
            "target": "create",
            "on": [{
                "name": "click",
                "emit": "newItem"
            }]
        }
    ],
    "html": "list.html",
    "css": ["//netdna.bootstrapcdn.com/twitter-bootstrap/2.3.1/css/bootstrap-combined.min.css"]
}
